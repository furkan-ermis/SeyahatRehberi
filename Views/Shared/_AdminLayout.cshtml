
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Admin Template</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc."
          name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Content/Panel/assets/images/favicon.ico" />

    <!-- App css -->

    <link href="~/Content/Panel/assets/css/app.min.css"
          rel="stylesheet"
          type="text/css"
          id="app-style" />

    <!-- icons -->
    <link href="~/Content/Panel/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
</head>

<!-- body start -->
<body class="loading"
      data-layout-color="dark"
      data-layout-mode="default"
      data-layout-size="fluid"
      data-topbar-color="dark"
      data-leftbar-position="fixed"
      data-leftbar-color="dark"
      data-leftbar-size="default"
      data-sidebar-user="true">
    <div id="wrapper">

        <div class="navbar-custom">
          
            <div class="clearfix"></div>
            <ul class="list-unstyled topnav-menu float-end mb-0 d-flex align-items-center list-unstyled topnav-menu topnav-menu-left mb-0">
                <li class="dropdown notification-list topbar-dropdown">
                    <a class="nav-link waves-effect waves-light"
                       href="@Url.Action("RaportedBlogs", "Admin")"
                       aria-haspopup="false"
                       aria-expanded="false">
                        <i class="fe-bell noti-icon" style="font-size:30px"></i>
                        <span class="badge bg-danger rounded-circle noti-icon-badge" style="font-size:1.3em;padding:3px;color:black !important;background-color:yellow !important">@ViewBag.RaportedBlogs</span>
                    </a>
                   
                </li>
               
                <li class="text-center ">
                    <a href="@Url.Action("Index", "Home")" class="btn btn-outline-danger nav-item">Home</a>
                    &nbsp;
                    &nbsp;
                </li>
                <li class="text-center ">
                    <a href="@Url.Action("Index", "Admin")" class="btn btn-outline-danger nav-item">Account</a>
                    &nbsp;
                    &nbsp;
                </li>
                <li>
                    <a href="@Url.Action("Logout", "Security")" class="btn  btn-outline-danger nav-item">Logout</a>
                </li>
            </ul>

            <ul class="list-unstyled topnav-menu topnav-menu-left mb-0">
                <li>
                    <button class="button-menu-mobile disable-btn waves-effect">
                        <i class="fe-menu"></i>
                    </button>
                </li>
                <li>
                    <h1 class="page-title-main">Welcome @ViewBag.Name</h1>
                </li>
            </ul>

            <div class="clearfix"></div>

        </div>
        <div class="left-side-menu">
            <div class="h-100" data-simplebar>
                <div class="user-box text-center">
                    <img src="@ViewBag.Image"
                         alt="user-img"
                         style="width: 100px; height: 100px"
                         title="Mat Helme"
                         class="rounded-circle img-thumbnail avatar-md" />
                    <ul class="list-inline">
                        <li class="list-inline-item">
                            <a href="@Url.Action("Logout", "Security")">
                                <i class="mdi mdi-power" style="font-size: 30px"></i>
                            </a>
                        </li>
                    </ul>
                </div>
                <div id="sidebar-menu">
                    <ul id="side-menu">
                        <li>
                            <a href="@Url.Action("Index", "Admin")">
                                <i class="mdi mdi-blogger"
                                   style="color: red; font-size: 25px"></i>&nbsp;
                                <span> Blogs </span>
                            </a>
                        </li>
                        <li>

                            <a href="@Url.Action("Users", "Admin")">
                                <i class="mdi mdi-account-hard-hat"
                                   style="color: red; font-size: 25px"></i>&nbsp;
                                <span> Users </span>
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("GetAwaitingCommentList", "Admin")">
                                <i class="mdi mdi-comment-alert-outline"
                                   style="color: red; font-size: 25px"></i>&nbsp;
                                <span>Comments awaiting </span>
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
        <div class="content-page">
            <div class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <script src="~/Content/Panel/assets/libs/jquery/jquery.min.js"></script>
    <script src="~/Content/Panel/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/Panel/assets/libs/simplebar/simplebar.min.js"></script>
    <script src="~/Content/Panel/assets/libs/node-waves/waves.min.js"></script>
    <script src="~/Content/Panel/assets/libs/waypoints/lib/jquery.waypoints.min.js"></script>
    <script src="~/Content/Panel/assets/libs/jquery.counterup/jquery.counterup.min.js"></script>
    <script src="~/Content/Panel/assets/libs/feather-icons/feather.min.js"></script>
    <script src="~/Content/Panel/assets/libs/jquery-tabledit/jquery.tabledit.min.js"></script>
    <script src="~/Content/Panel/assets/js/pages/tabledit.init.js"></script>
    <script src="~/Content/Panel/assets/js/app.min.js"></script>
</body>

